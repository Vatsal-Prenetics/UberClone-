    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "tsc-files": "^1.1.3",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
